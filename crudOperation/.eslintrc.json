{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["prettier", "airbnb-base"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"prettier/prettier": ["error", { "singleQuote": true, "parser": "flow" }],
		"no-console": "off",
		"func-names": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off",
		"allowIndentationTabs": "true"
	}
}
